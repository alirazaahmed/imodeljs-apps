{
  "extends": "./node_modules/@bentley/build-tools/tsconfig-base.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./node_modules",
    "outDir": "./lib",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "exclude": [
    "lib",
    "node_modules",
    "test",
    "SampleApp-win32-x64",
    "LockReleaser-win32-x64",
    "src_workingState1"
  ]
}
